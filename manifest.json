{
    "manifest_version": 2,
    "name": "Shopaholic",
    "version": "1.0.0",
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js": ["Content.js"],
            "run_at": "document_start"
        } 
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "storage",
        "tabs"
    ]
}